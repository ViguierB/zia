{
	"listen": [
		8080,
		4433
	],
	"server": [
		{
			"host": "localhost",
			"port": 8080,
			"path": "www/def/",
			"mime": [
				[ ".test", "text/plain" ],
				[ ".txt", "text/plain" ],
				[ ".png", "image/png" ]
			]
		},
		{
			"host": "viguierb.localhost",
			"port": 4433,
			"path": "www/viguierb/",
			"ssl": {
				"certificate": "cert/server.pem",
				"private-key": "cert/server.pem"
			},
			"mime": [
				[ ".png", "image/png" ],
				[ ".jpg", "image/jpg" ],
				[ ".jpeg", "image/jpeg" ],
				[ ".php" ]
			]
		},
		{
			"host": "secure.localhost",
			"port": 4433,
			"rewrite": [
				{
					"if": [
						{
							"path": "/test"
						}
					],
					"var": [
						"salut"
					],
					"set": [
						{
							"salut": "test.php"
						}
					],
					"rule": "/$salut"
				}
			],
			"path": "www/def/",
			"ssl": {
				"certificate": "cert/server.pem",
				"private-key": "cert/server.pem"
			},
			"mime": [
				[ ".test", "text/plain" ],
				[ ".txt", "text/plain" ]
			]
		},
		{
			"host": "manager.localhost",
			"port": 4433,
			"path": "www/manager/",
			"ssl": {
				"certificate": "cert/server.pem",
				"private-key": "cert/server.pem"
			},
			"manager": {
				"password": "8c6976e5b5410415bde908bd4dee15dfb167a9c873fc4bb8a81f6f2ab448a918"
			},
			"headers": {
				"response": {
					"access-control-allow-origin": "*",
					"access-control-allow-headers": "manager-api, content-type",
					"access-control-allow-methods": "POST, GET, OPTION"
				}
			}
		},
		{
			"host": "reverse.localhost",
			"port": 8080,
			"reverse-proxy": {
				"host": "localhost",
				"port": 8090,
				"scheme": "http"
			}
		},
		{
			"host": "secure.reverse.localhost",
			"port": 4433,
			"ssl": {
				"certificate": "cert/server.pem",
				"private-key": "cert/server.pem"
			},
			"reverse-proxy": {
				"host": "holidev.net",
				"port": 8088,
				"scheme": "http"
			},
			"headers": {
				"request": {
					"host": "holidev.net:8088",
					"cache-control": "no-cache",
					"pragma": "no-cache"
				}
			}
		},
		{
			"host": "holidev.localhost",
			"port": 8080,
			"reverse-proxy": {
				"host": "www.holidev.com",
				"port": 443,
				"scheme": "https"
			}
		},
		{
			"host": "test.reverse.localhost",
			"port": 8080,
			"reverse-proxy": {
				"host": "localhost",
				"port": 8888,
				"scheme": "http"
			}
		}
	]
